{
  "manifest_version": 2,
  "name": "Google Image Change",
  "version": "1.0",
  "description": "This extensions alters the content of a Google images page.",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?q=*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/trippy.gif"
  ]
}
